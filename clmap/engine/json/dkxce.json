{	// version v1.4.0.15	Map:	{		Popup:		{			onPopup: function(sender){				// sender.menuitems = [];				sender.menuitems[1].checked = sender.clmap.debug;				sender.menuitems[2].checked = sender.clmap.GridIsOn;				sender.menuitems[4].checked = sender.clmap.nav_mode == 7;				sender.menuitems[4].hidden = false;				if(!(sender.clmap.GeoClickPoints)) sender.menuitems[4].hidden = true;				if(sender.clmap.ie) sender.menuitems[2].disable = true;			},			Items: 			[				// 0				// 5				{caption: 'Установка центра карты...',				func: function(sender){ sender.clmap.ShowSMCDlg(); }, hidden: false, disable: false, checked: false},								// 1				{caption: 'Отображать сетку картинок',				func: function(sender){					sender.clmap.ShowImgsBorderVis(!sender.clmap.debug);				},				hidden: false, disable: false, checked: false},								// 2				{caption: 'Отображать масштабную сетку',				func: function(sender){					sender.clmap.Grid(!sender.clmap.GridIsOn);						},				hidden: false, disable: false, checked: false},								// 3				{caption: '', func: function(sender){}, hidden: false, disable: true, checked: false},								// 4				{caption: 'Получение координат точек (инструмент)',				func: function(sender){					if(sender.clmap.nav_mode != 7)					{						sender.clmap.NavigateMode(7);						sender.clmap.GeoClickPoints();					}					else						sender.clmap.NavigateMode(1);				},				hidden: false, disable: false, checked: false}									]		}	}}