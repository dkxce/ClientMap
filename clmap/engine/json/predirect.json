{	Map:	{		Popup:		{			onPopup: function(sender){				// sender.menuitems = [];				sender.menuitems[0].disable = sender.clmap.nav_mode != 1;				sender.menuitems[1].checked = sender.clmap.GridIsOn;				if(sender.clmap.ie) sender.menuitems[1].disable = true;			},			Items: 			[				// 0				{caption: 'Получить координаты точки...', 				func: function(sender){						// sender.clmap //						// sender.clickpoint = {x,y} //						var tmp_x = sender.clickpoint.x - sender.clmap.map_width / 2 + sender.clmap.center_x;						var tmp_y = sender.clickpoint.y - sender.clmap.map_height / 2 + sender.clmap.center_y;						var tmp_w = sender.clmap.ConvertPointToLatLon(tmp_x*sender.clmap.MapZoom,-1*tmp_y*sender.clmap.MapZoom);						MessageDlg('Координаты точки','<br/><b>'+(tmp_w.lat >= 0 ? 'N' : 'S')+'</b> '+tmp_w.lat+'<br/><b>'+(tmp_w.lon >= 0 ? 'E' : 'W')+'</b> '+tmp_w.lon,2,['OK'],false);				}, 				hidden: false, disable: false, checked: false},								// 1				{caption: 'Отображать масштабную сетку',				func: function(sender){					sender.clmap.Grid(!sender.clmap.GridIsOn);						},				hidden: false, disable: false, checked: false}			]		}	}}